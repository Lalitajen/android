<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.my_flag">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/flag"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/flag"
        android:supportsRtl="true"
        android:theme="@style/Theme.My_flag">
        <activity
            android:name=".CustomListView"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".My_Gridview" />
        <activity android:name=".My_Listview" />
        <activity android:name=".GridGallery" />
        <activity android:name=".FullImageView"/>
        <activity android:name=".ListGallery"/>
    </application>

</manifest>